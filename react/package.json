        "axios": "^0.19.2",
        "babel-jest": "^25.2.6",
        "dotenv": "^8.2.0",
        "jwt-decode": "^2.2.0",
        "mongojs": "^3.1.0",
        "node-device-detector": "^1.2.2",
        "npm-check-updates": "^4.0.1",
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-color": "^2.18.0",
        "react-color-picker": "^4.0.2",
        "react-device-detect": "^1.11.14",
        "react-dom": "^16.12.0",
        "react-google-maps": "^9.4.5",
        "react-places-autocomplete": "^7.2.1",
